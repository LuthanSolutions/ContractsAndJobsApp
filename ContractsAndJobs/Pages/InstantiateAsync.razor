@page "/InstantiateAsync"
<h3>Instantiate Async</h3>


<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @if (ViewModel?.People != null)
        {
            foreach(var person in ViewModel.People)
            {
                <tr>
                    <td>@person.FirstName</td>
                    <td>@person.LastName</td>
                </tr>
            }
        }
    </tbody>
</table>
